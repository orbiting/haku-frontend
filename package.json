{
  "name": "publikator-frontend",
  "version": "1.0.0",
  "engines": {
    "node": "10.4.1"
  },
  "description": "",
  "main": "server/index.js",
  "scripts": {
    "dev": "nodemon --watch server --watch app --watch lib/routes.js server/index.js",
    "start": "node server/index.js",
    "build": "next build",
    "heroku-postbuild": "npm run build",
    "tape": "cross-env NODE_ENV=test babel-tape-runner",
    "test": "npm run test:unit && npm run test:jsdom",
    "test:unit": "cross-env NODE_ENV=test babel-tape-runner \"?(lib|pages|components)/**/*.test.js\" | faucet",
    "test:jsdom": "cross-env NODE_ENV=test babel-tape-runner test/jsdom/setup.js \"test/jsdom/**/*.test.js\" | faucet",
    "cloc": "find . -name '*.js' -not -path \"./node_modules*\" -not -path \"./.next*\" | xargs wc -l",
    "standardize": "standard --fix",
    "precommit": "npm run test:unit && lint-staged",
    "translations": "gsheets --key=1qPubCKz2YzbcFkRfF0MrgRqD0KLsgRPxoZfG5G3tHOA --title=live --pretty --out lib/translations.json",
    "preinstall": "rm -rf .next && rm -rf node_modules/glamor && rm -rf node_modules/@project-r/styleguide",
    "link:sg": "rm -rf .next && npm link @project-r/styleguide && rm -rf node_modules/glamor && ln -s @project-r/styleguide/node_modules/glamor node_modules/glamor && echo '\n⚠️  Make sure to restart the next.js server. To unlink simply run npm install.\n'",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orbiting/haku-frontend.git"
  },
  "author": "",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/orbiting/haku-frontend/issues"
  },
  "homepage": "https://github.com/orbiting/haku-frontend#readme",
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-tape-runner": "^2.0.1",
    "cross-env": "^5.1.1",
    "diff": "^3.4.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint-plugin-import": "^2.7.0",
    "faucet": "0.0.1",
    "gsheets": "^2.0.0",
    "husky": "^0.14.3",
    "jsdom": "^11.7.0",
    "lint-staged": "^4.0.4",
    "nodemon": "^1.11.0",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.1.1",
    "spy": "^1.0.0",
    "standard": "^10.0.3",
    "tape": "^4.8.0"
  },
  "dependencies": {
    "@orbiting/remark-preset": "^1.2.1",
    "@project-r/styleguide": "^5.80.1",
    "@project-r/template-newsletter": "^1.5.0",
    "d3-array": "^1.2.1",
    "d3-color": "^1.0.3",
    "d3-dsv": "^1.0.8",
    "d3-format": "^1.2.2",
    "d3-scale": "^1.0.7",
    "d3-shape": "^1.2.0",
    "d3-time": "^1.0.7",
    "d3-time-format": "^2.1.1",
    "debug": "^2.6.9",
    "dotenv": "^4.0.0",
    "downshift": "^1.18.0",
    "email-validator": "^1.1.1",
    "express": "^4.15.4",
    "express-basic-auth": "^1.1.1",
    "glamor": "^2.20.39",
    "immutable": "^3.8.1",
    "immutable-treeutils": "^1.1.0",
    "is-url": "^1.2.2",
    "js-yaml": "^3.9.1",
    "lodash.debounce": "^4.0.8",
    "mdast-react-render": "^1.1.0",
    "next": "^4.2.3",
    "next-routes": "^1.2.0",
    "prop-types": "^15.6.0",
    "ramda": "^0.24.1",
    "react": "^16.1.1",
    "react-apollo": "^1.4.15",
    "react-color": "^2.13.8",
    "react-dnd-html5-backend": "^2.5.4",
    "react-dom": "^16.1.1",
    "react-frame-component": "^1.1.1",
    "react-icons": "^2.2.5",
    "react-maskedinput": "^4.0.0",
    "react-portal": "^3.1.0",
    "react-textarea-autosize": "^5.1.0",
    "redux": "^3.7.2",
    "remark-frontmatter": "^1.1.0",
    "remark-parse": "^4.0.0",
    "remark-stringify": "^4.0.0",
    "slate": "^0.31.8",
    "slate-mdast-serializer": "^2.0.0",
    "slate-react": "^0.10.23",
    "subscriptions-transport-ws": "^0.8.2",
    "unified": "^6.1.5",
    "snyk": "^1.83.0"
  },
  "snyk": true
}
